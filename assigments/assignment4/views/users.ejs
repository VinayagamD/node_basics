<%- include('includes/head.ejs') %>
<link rel="stylesheet" href="/css/product.css">
<%- include('includes/navigation.ejs')%>

<main>
    <h1>My Users</h1>
    <% if(users.length > 0) { %>
        <div class="grid">
            <ul>
            <% for(let user of users){%>
                <li><%=user.name%></li>
            <% }%>
            </ul>
        </div>
    <%} else  {%>
        <h1> Users Not Found</h1>
    <%}%>
</main>
<%- include('includes/end.ejs')%>
